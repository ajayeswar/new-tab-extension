{
  "manifest_version": 2,

  "name": "ROOKIE NEW TAB EXTENSION",
  "description": "ROOKIE NEW TAB EXTENSION",
  "version": "0.0.1",
  "browser_action": {
    "default_title": "ROOKIE NEW TAB EXTENSION",
    "default_icon": {
      "16": "people-1.png",
      "32": "people-1.png",
      "48": "people-1.png",
      "64": "people-1.png"
   }
  },
  "icons": {
    "16": "people-1.png",
    "48": "people-1.png",
    "128": "people-1.png"
  },
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "content_scripts" : [
    {
        "matches" : [ "<all_urls>" ],
        "js": ["contentScript.js","/static/js/2.js", "/static/js/3.js"],
        "run_at": "document_end"
    }
  ],
  "web_accessible_resources":[
    "contentScript.js"
 ], 
  "content_security_policy": "script-src 'self' 'sha256-zsY8slItr1UXNrHaMiKzQxEqIMjRDrGN9bJnpePocH4='; object-src 'self'",
  "permissions": ["tabs","topSites", "bookmarks", "activeTab", "cookies", "*://*.localhost/*", "notifications", "storage", "downloads"]

}